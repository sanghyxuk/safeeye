<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 테두리 -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#000000" /> <!-- 검정색 테두리 -->
        </shape>
    </item>

    <!-- 내부 -->
    <item
        android:left="5dp"
        android:top="5dp"
        android:right="5dp"
        android:bottom="5dp">
        <shape android:shape="oval">
            <solid android:color="#FFFFFF" /> <!-- 흰색 내부 -->
        </shape>
    </item>
</layer-list>
